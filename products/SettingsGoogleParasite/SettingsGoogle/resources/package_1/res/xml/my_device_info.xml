<?xml version='1.0' encoding='utf-8' ?>
<PreferenceScreen android:title="@string/about_settings"
                  android:key="my_device_info_pref_screen" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:settings="http://schemas.android.com/apk/res-auto">
  <com.android.settingslib.widget.LayoutPreference android:layout="@layout/settings_entity_header"
                        android:selectable="false"
                        android:key="my_device_info_header"
                        android:order="0"
                        settings:isPreferenceVisible="false" />
  <PreferenceCategory android:title="@string/my_device_info_basic_info_category_title"
                      android:selectable="false"
                      android:key="basic_info_category">
    <com.android.settings.widget.ValidatedEditTextPreference android:title="@string/my_device_info_device_name_preference_title"
                          android:key="device_name"
                          android:summary="@string/summary_placeholder"
                          android:order="1"
                          settings:controller="com.android.settings.deviceinfo.DeviceNamePreferenceController"
                          settings:enableCopying="true" />
    <Preference android:title="@string/my_device_info_account_preference_title"
                android:key="branded_account"
                android:summary="@string/summary_placeholder"
                android:order="2"
                settings:controller="com.android.settings.deviceinfo.BrandedAccountPreferenceController"
                settings:enableCopying="true" />
    <com.android.settings.deviceinfo.PhoneNumberSummaryPreference android:title="@string/status_number"
                          android:key="phone_number"
                          android:summary="@string/summary_placeholder"
                          android:order="3"
                          settings:controller="com.android.settings.deviceinfo.PhoneNumberPreferenceController"
                          settings:isPreferenceVisible="@bool/config_show_sim_info"
                          settings:enableCopying="true" />
  </PreferenceCategory>
  <PreferenceCategory android:title="@string/my_device_info_legal_category_title">
    <Preference android:title="@string/legal_information"
                android:key="legal_container"
                android:order="15"
                android:fragment="com.android.settings.LegalSettings" />
    <Preference android:title="@string/regulatory_labels"
                android:key="regulatory_info"
                android:order="16">
      <intent android:action="android.settings.SHOW_REGULATORY_INFO" />
    </Preference>
    <Preference android:title="@string/limited_warranty_title"
                android:key="limited_warranty"
                android:order="17"
                settings:controller="com.google.android.settings.warranty.WarrantyPreferenceController"
                settings:keywords="@string/keywords_limited_warranty">
      <intent android:action="android.settings.WARRANTY" />
    </Preference>
    <Preference android:title="@string/safety_and_regulatory_info"
                android:key="safety_info"
                android:order="18" />
  </PreferenceCategory>
  <PreferenceCategory android:title="@string/my_device_info_device_details_category_title"
                      android:selectable="false"
                      android:key="device_detail_category">
    <Preference android:title="@string/sim_status_title"
                android:key="sim_status"
                android:summary="@string/summary_placeholder"
                android:order="19"
                settings:controller="com.android.settings.deviceinfo.simstatus.SimStatusPreferenceController"
                settings:keywords="@string/keywords_sim_status"
                settings:isPreferenceVisible="@bool/config_show_sim_info" />
    <Preference android:title="@string/model_info"
                android:key="device_model"
                android:summary="@string/summary_placeholder"
                android:order="30"
                android:fragment="com.android.settings.deviceinfo.hardwareinfo.HardwareInfoFragment"
                settings:controller="com.android.settings.deviceinfo.HardwareInfoPreferenceController"
                settings:keywords="@string/keywords_model_and_hardware"
                settings:enableCopying="true" />
    <com.android.settings.network.telephony.TelephonyPreferenceDialog android:title="@string/status_eid"
                          android:key="eid_info"
                          android:summary="@string/device_info_protected_single_press"
                          android:order="31"
                          android:positiveButtonText="@string/dlg_ok"
                          android:dialogLayout="@layout/dialog_eid_status"
                          settings:controller="com.android.settings.deviceinfo.simstatus.SimEidPreferenceController"
                          settings:isPreferenceVisible="@bool/config_show_sim_info"
                          settings:enableCopying="true" />
    <com.android.settings.deviceinfo.PhoneNumberSummaryPreference android:title="@string/status_imei"
                          android:key="imei_info"
                          android:summary="@string/summary_placeholder"
                          android:order="32"
                          settings:controller="com.android.settings.deviceinfo.imei.ImeiInfoPreferenceController"
                          settings:keywords="@string/keywords_imei_info"
                          settings:isPreferenceVisible="@bool/config_show_sim_info"
                          settings:enableCopying="true" />
    <Preference android:title="@string/firmware_version"
                android:key="firmware_version"
                android:summary="@string/summary_placeholder"
                android:order="42"
                android:fragment="com.android.settings.deviceinfo.firmwareversion.FirmwareVersionSettings"
                settings:controller="com.android.settings.deviceinfo.firmwareversion.FirmwareVersionPreferenceController" />
    <Preference android:title="@string/battery_info"
                android:key="battery_info"
                android:order="43"
                android:fragment="com.android.settings.deviceinfo.batteryinfo.BatteryInfoFragment"
                settings:keywords="@string/keywords_battery_info" />
  </PreferenceCategory>
  <PreferenceCategory android:title="@string/my_device_info_device_identifiers_category_title">
    <Preference android:title="@string/wifi_ip_address"
                android:selectable="false"
                android:key="wifi_ip_address"
                android:summary="@string/summary_placeholder"
                android:order="44"
                settings:enableCopying="true" />
    <Preference android:title="@string/status_wifi_mac_address"
                android:key="saved_accesspoints_wifi_mac_address"
                android:summary="@string/view_saved_network"
                android:order="45"
                android:fragment="com.android.settings.wifi.savedaccesspoints2.SavedAccessPointsWifiSettings2"
                settings:enableCopying="false" />
    <Preference android:title="@string/status_device_wifi_mac_address"
                android:selectable="false"
                android:key="wifi_mac_address"
                android:summary="@string/summary_placeholder"
                android:order="46"
                settings:enableCopying="true" />
    <Preference android:title="@string/status_bt_address"
                android:selectable="false"
                android:key="bt_address"
                android:summary="@string/summary_placeholder"
                android:order="47"
                settings:enableCopying="true" />
    <Preference android:title="@string/status_up_time"
                android:selectable="false"
                android:key="up_time"
                android:summary="@string/summary_placeholder"
                android:order="48" />
    <Preference android:title="@string/manual"
                android:key="manual"
                android:order="50">
      <intent android:action="android.settings.SHOW_MANUAL" />
    </Preference>
    <Preference android:title="@string/device_feedback"
                android:key="device_feedback"
                android:order="51"
                settings:keywords="@string/keywords_device_feedback" />
    <Preference android:title="@string/fcc_equipment_id"
                android:key="fcc_equipment_id"
                android:summary="@string/summary_placeholder"
                android:order="52" />
    <Preference android:title="@string/build_number"
                android:key="build_number"
                android:summary="@string/summary_placeholder"
                android:order="53"
                settings:controller="com.android.settings.deviceinfo.BuildNumberPreferenceController"
                settings:enableCopying="true" />
  </PreferenceCategory>
</PreferenceScreen>