<?xml version='1.0' encoding='utf-8' ?>
<FrameLayout android:id="@id/device_container"
             android:layout_width="fill_parent"
             android:layout_height="wrap_content" xmlns:android="http://schemas.android.com/apk/res/android">
  <FrameLayout android:id="@id/item_layout"
               android:background="@drawable/media_output_item_background"
               android:layout_width="fill_parent"
               android:layout_height="64.0dp"
               android:layout_marginBottom="12.0dp"
               android:layout_marginStart="16.0dp"
               android:layout_marginEnd="80.0dp">
    <FrameLayout android:layout_gravity="center|start"
                 android:layout_width="fill_parent"
                 android:layout_height="fill_parent">
      <com.android.systemui.media.dialog.MediaOutputSeekbar android:id="@id/volume_seekbar"
                            android:background="@null"
                            android:visibility="gone"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:progressDrawable="@drawable/media_output_dialog_seekbar_background"
                            android:thumb="@null"
                            android:contentDescription="@string/media_output_dialog_accessibility_seekbar"
                            android:paddingStart="64.0dp"
                            android:paddingEnd="0.0dp"
                            android:splitTrack="false" />
    </FrameLayout>
    <FrameLayout android:layout_gravity="center|start"
                 android:id="@id/icon_area"
                 android:background="@drawable/media_output_title_icon_area"
                 android:focusable="false"
                 android:layout_width="64.0dp"
                 android:layout_height="64.0dp"
                 android:importantForAccessibility="no">
      <ImageView android:layout_gravity="center"
                 android:id="@id/title_icon"
                 android:focusable="false"
                 android:layout_width="24.0dp"
                 android:layout_height="24.0dp"
                 android:animateLayoutChanges="true"
                 android:importantForAccessibility="no" />
      <TextView android:textSize="16.0sp"
                android:layout_gravity="center"
                android:id="@id/volume_value"
                android:focusable="false"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:animateLayoutChanges="true"
                android:importantForAccessibility="no"
                android:fontFamily="@android:string/config_inCallNotificationSound" />
    </FrameLayout>
    <TextView android:textSize="16.0sp"
              android:ellipsize="end"
              android:layout_gravity="center|start"
              android:id="@id/title"
              android:focusable="false"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:maxLines="1"
              android:importantForAccessibility="no"
              android:fontFamily="@android:string/config_inCallNotificationSound"
              android:layout_marginStart="72.0dp"
              android:layout_marginEnd="56.0dp" />
    <LinearLayout android:layout_gravity="center|start"
                  android:orientation="vertical"
                  android:id="@id/two_line_layout"
                  android:layout_width="wrap_content"
                  android:layout_height="48.0dp"
                  android:layout_marginStart="72.0dp"
                  android:layout_marginEnd="56.0dp">
      <TextView android:textSize="16.0sp"
                android:textColor="@color/media_dialog_item_main_content"
                android:ellipsize="end"
                android:id="@id/two_line_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:maxLines="1"
                android:fontFamily="@android:string/config_inCallNotificationSound" />
      <TextView android:textSize="14.0sp"
                android:textColor="@color/media_dialog_item_main_content"
                android:ellipsize="marquee"
                android:id="@id/subtitle"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:maxLines="1"
                android:singleLine="true"
                android:marqueeRepeatLimit="marquee_forever"
                android:fontFamily="@android:string/config_cameraShutterSound" />
    </LinearLayout>
    <ProgressBar android:layout_gravity="center|end"
                 android:id="@id/volume_indeterminate_progress"
                 android:visibility="gone"
                 android:layout_width="24.0dp"
                 android:layout_height="24.0dp"
                 android:indeterminate="true"
                 android:indeterminateOnly="true"
                 android:layout_marginEnd="16.0dp"
                 style="?android:attr/progressBarStyleSmallTitle" />
    <ImageView android:layout_gravity="center|end"
               android:id="@id/media_output_item_status"
               android:visibility="gone"
               android:layout_width="24.0dp"
               android:layout_height="24.0dp"
               android:indeterminate="true"
               android:indeterminateOnly="true"
               android:importantForAccessibility="no"
               android:layout_marginEnd="16.0dp" />
  </FrameLayout>
  <FrameLayout android:gravity="center"
               android:layout_gravity="center|end"
               android:id="@id/end_action_area"
               android:background="@drawable/media_output_item_background_active"
               android:visibility="gone"
               android:layout_width="64.0dp"
               android:layout_height="64.0dp"
               android:layout_marginBottom="6.0dp"
               android:layout_marginEnd="8.0dp">
    <CheckBox android:layout_gravity="center"
              android:id="@id/check_box"
              android:focusable="false"
              android:visibility="gone"
              android:layout_width="24.0dp"
              android:layout_height="24.0dp"
              android:button="@drawable/media_output_item_check_box"
              android:importantForAccessibility="no" />
    <ImageView android:layout_gravity="center"
               android:id="@id/media_output_item_end_click_icon"
               android:focusable="false"
               android:visibility="gone"
               android:layout_width="24.0dp"
               android:layout_height="24.0dp"
               android:indeterminate="true"
               android:indeterminateOnly="true"
               android:importantForAccessibility="no" />
  </FrameLayout>
</FrameLayout>