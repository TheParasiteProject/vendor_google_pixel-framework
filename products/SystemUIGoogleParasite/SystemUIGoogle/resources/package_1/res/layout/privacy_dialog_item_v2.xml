<?xml version='1.0' encoding='utf-8' ?>
<androidx.cardview.widget.CardView android:id="@id/privacy_dialog_item_card"
                      android:layout_width="fill_parent"
                      android:layout_height="wrap_content"
                      android:layout_marginTop="4.0dp"
                      android:layout_marginBottom="4.0dp"
                      android:foreground="?android:attr/selectableItemBackground"
                      systemui:cardBackgroundColor="?android:^attr-private/materialColorSurfaceContainerHighest"
                      systemui:cardCornerRadius="28.0dp"
                      systemui:cardElevation="0.0dp" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:systemui="http://schemas.android.com/apk/res-auto">
  <LinearLayout android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">
    <LinearLayout android:orientation="horizontal"
                  android:id="@id/privacy_dialog_item_header"
                  android:paddingTop="20.0dp"
                  android:paddingBottom="20.0dp"
                  android:layout_width="fill_parent"
                  android:layout_height="wrap_content"
                  android:paddingStart="24.0dp"
                  android:paddingEnd="24.0dp">
      <ImageView android:id="@id/privacy_dialog_item_header_icon"
                 android:layout_width="wrap_content"
                 android:layout_height="fill_parent"
                 android:layout_centerVertical="true"
                 android:importantForAccessibility="no" />
      <LinearLayout android:orientation="vertical"
                    android:layout_width="0.0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="1.0"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="16.0dp"
                    android:layout_marginEnd="16.0dp">
        <TextView android:textAppearance="@style/TextAppearance.PrivacyDialog.Item.Title"
                  android:id="@id/privacy_dialog_item_header_title"
                  android:layout_width="fill_parent"
                  android:layout_height="wrap_content"
                  android:layout_marginBottom="2.0dp"
                  android:textAlignment="viewStart"
                  android:hyphenationFrequency="normalFast" />
        <TextView android:textAppearance="@style/TextAppearance.PrivacyDialog.Item.Summary"
                  android:id="@id/privacy_dialog_item_header_summary"
                  android:layout_width="fill_parent"
                  android:layout_height="wrap_content"
                  android:textAlignment="viewStart" />
      </LinearLayout>
      <ImageView android:id="@id/privacy_dialog_item_header_expand_toggle"
                 android:visibility="gone"
                 android:layout_width="wrap_content"
                 android:layout_height="fill_parent"
                 android:layout_centerVertical="true" />
    </LinearLayout>
    <LinearLayout android:orientation="vertical"
                  android:id="@id/privacy_dialog_item_header_expanded_layout"
                  android:paddingBottom="16.0dp"
                  android:visibility="gone"
                  android:layout_width="fill_parent"
                  android:layout_height="wrap_content"
                  android:paddingStart="24.0dp"
                  android:paddingEnd="24.0dp" />
  </LinearLayout>
</androidx.cardview.widget.CardView>