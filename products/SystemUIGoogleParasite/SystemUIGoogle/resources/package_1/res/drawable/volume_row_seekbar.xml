<?xml version='1.0' encoding='utf-8' ?>
<layer-list android:paddingMode="stack" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:androidprv="http://schemas.android.com/apk/prv/res/android">
  <item android:gravity="center|fill_horizontal"
        android:id="@android:id/background">
    <shape>
      <size android:height="@dimen/volume_dialog_track_width" />
      <corners android:radius="@dimen/volume_dialog_panel_width_half" />
      <solid android:color="?android:^attr-private/materialColorPrimaryFixed" />
    </shape>
  </item>
  <item android:gravity="center|fill_horizontal"
        android:id="@android:id/progress">
    <com.android.systemui.util.RoundedCornerProgressDrawable android:drawable="@drawable/volume_row_seekbar_progress" />
  </item>
</layer-list>