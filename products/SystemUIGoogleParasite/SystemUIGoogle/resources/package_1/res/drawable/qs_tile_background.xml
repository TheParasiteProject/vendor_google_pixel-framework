<?xml version='1.0' encoding='utf-8' ?>
<ripple android:color="@color/qs_tile_ripple_color" xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:id="@android:id/mask"
        android:drawable="@drawable/qs_tile_background_shape" />
  <item android:id="@id/background">
    <layer-list>
      <item android:id="@id/qs_tile_background_base"
            android:drawable="@drawable/qs_tile_background_shape" />
      <item android:id="@id/qs_tile_background_overlay">
        <selector>
          <item android:drawable="@drawable/qs_tile_background_shape"
                android:state_hovered="true" />
          <item android:state_focused="true"
                android:drawable="@drawable/qs_tile_background_shape" />
        </selector>
      </item>
    </layer-list>
  </item>
</ripple>