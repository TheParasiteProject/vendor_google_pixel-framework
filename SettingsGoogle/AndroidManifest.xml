                <action android:name="com.google.android.settings.action.RESTORE_APP_BYPASS_SETTINGS" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="com.google.android.settings.vpn2.AppBypassNotificationBroadcastReceiver"
