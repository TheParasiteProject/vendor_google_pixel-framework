<?xml version='1.0' encoding='UTF-8'?>
<resources xmlns:androidprv="http://schemas.android.com/apk/prv/res/android">
    <style name="DateWeatherText" parent="@style/EnhancedSmartspaceTextTitle">
        <item name="android:textSize">?smartspaceSubtitleTextSize</item>
        <item name="android:fontFamily">google-sans-text</item>
    </style>
    <style name="DotStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:alpha">0.0</item>
    </style>
    <style name="EnhancedSmartspaceTextSubtitle" parent="@style/EnhancedSmartspaceTextTitle">
        <item name="android:textSize">?smartspaceSubtitleTextSize</item>
        <item name="android:fontFamily">google-sans-text</item>
        <item name="android:letterSpacing">@dimen/smartspaceLetterSpacing</item>
    </style>
    <style name="EnhancedSmartspaceTextTitle" parent="@android:style/Theme.DeviceDefault.DayNight">
        <item name="android:textSize">?smartspaceTitleTextSize</item>
        <item name="android:textColor">?android:textColorPrimary</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:maxLines">1</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:drawablePadding">@dimen/enhanced_smartspace_icon_margin</item>
        <item name="android:fontFamily">google-sans</item>
        <item name="android:lineHeight">24.0sp</item>
    </style>
    <style name="LockscreenDotStyle" parent="@style/DotStyle">
        <item name="android:alpha">0.0</item>
        <item name="android:scaleX">0.8</item>
        <item name="android:scaleY">0.8</item>
    </style>
    <style name="ScreenProtectorNotification"/>
    <style name="ScreenProtectorNotification.UserAction" parent="@style/ScreenProtectorNotification">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">?android:textColorPrimary</item>
        <item name="android:background">?android:selectableItemBackground</item>
        <item name="android:paddingLeft">16.0dip</item>
        <item name="android:paddingTop">16.0dip</item>
        <item name="android:paddingRight">24.0dip</item>
        <item name="android:paddingBottom">16.0dip</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">roboto-regular</item>
        <item name="android:textAlignment">viewStart</item>
    </style>
    <style name="Smartspace">
        <item name="smartspaceSubtitleTextSize">@dimen/enhanced_smartspace_text_size_medium</item>
        <item name="smartspaceTitleTextSize">@dimen/enhanced_smartspace_text_size_regular</item>
    </style>
    <style name="TrimmedHorizontalProgressBar" parent="@android:style/Widget.Material.ProgressBar.Horizontal">
        <item name="android:maxHeight">3.0dip</item>
        <item name="android:indeterminateDrawable">@drawable/progress_indeterminate_horizontal_material_trimmed</item>
        <item name="android:minHeight">3.0dip</item>
    </style>
</resources>